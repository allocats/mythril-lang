struct MyStruct {
    a: i32;
    b: i32;
}

impl MyStruct {
    // All structs are intialized to zero, this leads to 0 * 2 
    fn do_nothing(self): i32 {
        self.a *= 2;
        return self.a;
    }

    fn do_something(&self): i32 {
        self -> b += 2;
        return self -> b;
    }
}

fn main(argc: i32, argv: char*[]): void {
    let my_struct: MyStruct;
    
    let a: i32 = my_struct.do_nothing();
    let b: i32 = my_struct.do_something();

    println("a=% b=%", a, b);
}
